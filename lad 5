#include <stdio.h>
#define MAX 5
int queue_array[MAX];
int rear = - 1;
int front = - 1;
void display()
{
int i;
if (front == - 1)
printf("\nQueue is empty \n");
else
{
printf("\nQueue is : ");
for (i = front; i <= rear; i++)
printf("%d ", queue_array[i]);
}
}
void insert_Q(int item)
{
if (rear == MAX - 1)
printf("\nQueue Overflow \n");
else
{
if (front == - 1)
front = 0;
rear = rear + 1;
queue_array[rear] = item;
}
display();
}
void delete_Q()
{
if (front == - 1 || front > rear)
{
printf("\nQueue Underflow");
return ;
}
else
{
printf("\nElement deleted from queue is : %d", queue_array[front]);
front = front + 1;
}
display();
}
void main()
{
int choice;
clrscr();
insert_Q(45);
insert_Q(34);
insert_Q(10);
insert_Q(63);
insert_Q(3);
insert_Q(0);
delete_Q();
delete_Q();
delete_Q();
delete_Q();
delete_Q();
delete_Q();
getch();
}