#include<stdio.h>
#include<conio.h>
#include<string.h>
int bubble(int a[],int n)
{
int pass,temp,j,i;
for(pass=1;pass<=n-1;pass++)
{
for(j=0;j<=n-pass-1;j++)
{
if(a[j]>a[j+1])
{
temp=a[j];
a[j]=a[j+1];
a[j+1]=temp;
}
}
}
}
for(i=0;i<n;i++)
printf(" %d",a[i]);
void main()
{
int a[5];
int n,i;
clrscr();
printf("\n enter the number elements:");
scanf("%d",&n);
printf("\n enter the array elements: ");
for(i=0;i<n;i++)
scanf("%d",&a[i]);
printf("\n sorted array is:");
bubble(a,n);
getch();
}